// Save early to get ID
$new_id =3D $product-&gt;save();
if ( is_wp_error($new_id) || ! $new_id ) { $skipped++; continue; }
$product_id =3D $new_id;

// Taxonomies: categories / tags
if ( $data['categories'] !=3D=3D '' ) {
wp_wiw_assign_categories($product_id, $data['categories']);
}
if ( $data['tags'] !=3D=3D '' ) {
$tags =3D preg_split('/\s*[|,]\s*/', $data['tags']);
$tags =3D array_filter(array_map('trim', (array)$tags));
if ( ! empty($tags) ) {
wp_set_post_terms($product_id, $tags, 'product_tag', false);
}
}

// Attributes
if ( ! empty($data['attributes']) ) {
wp_wiw_assign_attributes($product_id, $data['attributes']);
}

// Images
if ( $opts['download_imgs'] ) {
if ( $data['image'] !=3D=3D '' ) {
$att_id =3D wp_wiw_attach_image_from_url($data['image'], $product_id, $opts=
['image_timeout']);
if ( $att_id ) { set_post_thumbnail($product_id, $att_id); $img_count++; }
}
if ( $data['gallery'] !=3D=3D '' ) {
$gallery_urls =3D preg_split('/\s*[|,]\s*/', $data['gallery']);
$gids =3D [];
foreach ( $gallery_urls as $url ) {
$aid =3D wp_wiw_attach_image_from_url(trim($url), $product_id, $opts['image=
_timeout']);
if ( $aid ) { $gids[] =3D $aid; $img_count++; }
}
if ( ! empty($gids) ) {
update_post_meta($product_id, '_product_image_gallery', implode(',', $gids)=
);
}
}
}

// Meta
if ( ! empty($data['meta']) ) {
foreach ($data['meta'] as $k =3D&gt; $v) {
update_post_meta($product_id, sanitize_key($k), $v);
}
}

// Count created/updated
if ( $product_id &amp;&amp; $data['sku'] &amp;&amp; wc_get_product_id_by_sk=
u($data['sku']) =3D=3D $product_id ) {
if ( get_post_meta($product_id, '_wiw_was_new', true) ) {
delete_post_meta($product_id, '_wiw_was_new');
$created++;
} else {
$updated++;
}
} else {
// No SKU path
// Heuristic: if post date is recent in this pass assume created
$post =3D get_post($product_id);
if ( $post &amp;&amp; ( time() - strtotime($post-&gt;post_date_gmt) ) &lt; =
60 ) $created++; else $updated++;
}
}

return compact('created','updated','skipped','img_count');
}

/**
* Normalize attribute name to Woo's attribute taxonomy format.
* Accepts "Color" or "pa_color" and returns slug "pa_color".
*/
function wp_wiw_normalize_attribute_name(string $name): string {
$name =3D trim($name);
$slug =3D wc_sanitize_taxonomy_name($name);
if ( strpos($slug, 'pa_') !=3D=3D 0 ) $slug =3D 'pa_' . $slug;
// Ensure attribute taxonomy exists (register if missing)
wp_wiw_ensure_global_attribute($name, $slug);
return $slug;
}

/**
* Make sure a global attribute exists (in wc_attribute_taxonomies)
*/
function wp_wiw_ensure_global_attribute(string $label, string $slug) {
global $wpdb;

$attr =3D $wpdb-&gt;get_row( $wpdb-&gt;prepare(
"SELECT * FROM {$wpdb-&gt;prefix}woocommerce_attribute_taxonomies WHERE att=
ribute_name =3D %s",
substr($slug, 3) // without pa_
) );

if ( ! $attr ) {
$wpdb-&gt;insert(
"{$wpdb-&gt;prefix}woocommerce_attribute_taxonomies",
[
'attribute_label' =3D&gt; $label,
'attribute_name' =3D&gt; substr($slug, 3),
'attribute_type' =3D&gt; 'select',
'attribute_orderby' =3D&gt; 'menu_order',
'attribute_public' =3D&gt; 1,
]
);
// Re-register taxonomies
delete_transient('wc_attribute_taxonomies');
wc_register_taxonomy( $slug, 'product' );
// Flush to be safe once
flush_rewrite_rules(false);
}
}

/**
* Assign attribute values to product (creates terms if needed)
*/
function wp_wiw_assign_attributes(int $product_id, array $attributes) {
$product =3D wc_get_product($product_id);
if ( ! $product ) return;

$wc_attrs =3D [];

foreach ($attributes as $attr_slug =3D&gt; $info) {
$label =3D $info['label'];
$vals =3D $info['values'];

// Ensure taxonomy
if ( taxonomy_exists($attr_slug) ) {
// Create terms if needed and assign
$term_ids =3D [];
foreach ($vals as $v) {
$term =3D term_exists($v, $attr_slug);
if ( ! $term ) {
$term =3D wp_insert_term($v, $attr_slug);
}
if ( ! is_wp_error($term) ) {
$term_ids[] =3D (int) ($term['term_id'] ?? $term['term_taxonomy_id']);
}
}
if ( ! empty($term_ids) ) {
wp_set_post_terms($product_id, $vals, $attr_slug, false);
}

$attribute =3D new WC_Product_Attribute();
$attribute-&gt;set_id( wc_attribute_taxonomy_id_by_name($attr_slug) );
$attribute-&gt;set_name( $attr_slug );
$attribute-&gt;set_options( $term_ids ); // taxonomy terms
$attribute-&gt;set_visible( true );
$attribute-&gt;set_variation( false );

$wc_attrs[] =3D $attribute;

} else {
// Custom product-level attribute (non-taxonomy)
$attribute =3D new WC_Product_Attribute();
$attribute-&gt;set_id( 0 );
$attribute-&gt;set_name( $label );
$attribute-&gt;set_options( $vals ); // plain strings
$attribute-&gt;set_visible( true );
$attribute-&gt;set_variation( false );

$wc_attrs[] =3D $attribute;
}
}

if ( ! empty($wc_attrs) ) {
$product-&gt;set_attributes( $wc_attrs );
$product-&gt;save();
}
}

/**
* Assign hierarchical categories. Supports:
* - "Parent &gt; Child &gt; Subchild"
* - Multiple categories separated by comma or pipe.
*/
function wp_wiw_assign_categories(int $product_id, string $raw) {
$chunks =3D preg_split('/\s*[|,]\s*/', $raw);
$cat_ids =3D [];

foreach ( $chunks as $chunk ) {
$hier =3D array_map('trim', explode('&gt;', $chunk));
$parent =3D 0; $term_id =3D 0;

foreach ( $hier as $name ) {
if ( $name =3D=3D=3D '' ) continue;
$term =3D term_exists($name, 'product_cat', $parent);
if ( ! $term ) {
$term =3D wp_insert_term($name, 'product_cat', ['parent' =3D&gt; $parent]);
}
if ( is_wp_error($term) ) continue;
$term_id =3D (int) $term['term_id'];
$parent =3D $term_id;
}

if ( $term_id ) $cat_ids[] =3D $term_id;
}

if ( ! empty($cat_ids) ) {
wp_set_post_terms($product_id, $cat_ids, 'product_cat', false);
}
}

/**
* Download an image from URL and attach to post
*/
function wp_wiw_attach_image_from_url(string $url, int $post_id, int $timeo=
ut =3D 25) {
$url =3D trim($url);
if ( $url =3D=3D=3D '' ) return 0;

$tmp =3D download_url($url, $timeout);
if ( is_wp_error($tmp) ) return 0;

$file_array =3D [
'name' =3D&gt; basename(parse_url($url, PHP_URL_PATH) ?: 'image.jpg'),
'tmp_name' =3D&gt; $tmp,
];

$id =3D media_handle_sideload($file_array, $post_id);
if ( is_wp_error($id) ) {
@unlink($file_array['tmp_name']);
return 0;
}
return $id;
}
