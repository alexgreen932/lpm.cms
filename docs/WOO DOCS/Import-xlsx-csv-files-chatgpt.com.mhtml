/**
* Read rows from CSV/XLSX
*/
function wp_wiw_read_rows(string $file_path, string $ext): array {
$rows =3D [];

if ( $ext =3D=3D=3D 'csv' ) {
$fh =3D fopen($file_path, 'r');
if ( ! $fh ) return [];
$headers =3D fgetcsv($fh);
if ( ! $headers ) return [];
while ( ($r =3D fgetcsv($fh)) !=3D=3D false ) {
// pad row length
$r =3D array_pad($r, count($headers), '');
$rows[] =3D array_combine($headers, $r);
}
fclose($fh);
} elseif ( $ext =3D=3D=3D 'xlsx' &amp;&amp; class_exists('SimpleXLSX') ) {
$xlsx =3D SimpleXLSX::parse($file_path);
if ( ! $xlsx ) return [];
$sheet =3D $xlsx-&gt;rows();
if ( empty($sheet) || empty($sheet[0]) ) return [];
$headers =3D $sheet[0];
for ($i =3D 1; $i &lt; count($sheet); $i++) {
$row =3D $sheet[$i];
$row =3D array_pad($row, count($headers), '');
$rows[] =3D array_combine($headers, $row);
}
}
return $rows;
}

