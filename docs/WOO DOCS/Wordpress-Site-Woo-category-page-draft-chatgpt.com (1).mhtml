/* =3D=3D=3D=3D=3D=3D=3D Single Product =E2=80=93 Variables =3D=3D=3D=3D=3D=
=3D=3D */
$pd-gap: 1rem;
$pd-col-left: 520px; // main image width
$pd-col-right: 340px; // buy box width
$pd-sticky-top: 80px; // sticky offset
$pd-radius: 16px;
$pd-shadow: 0 12px 36px rgba(0, 0, 0, .35);

$pd-bg: #0f0f10;
$pd-card: #141416;
$pd-text: #fff;
$pd-muted: rgba(255, 255, 255, .7);
$pd-soft: rgba(255, 255, 255, .55);

$pd-accent: #ff4da6; // price &amp; CTA accent
$pd-oldprice: rgba(255, 255, 255, .7);
$pd-border: #242428;
$l:40%;
$c:30%;
$r:30%;

/* =3D=3D=3D=3D=3D=3D=3D Layout =3D=3D=3D=3D=3D=3D=3D */
.site-main.d-product {
// background: $pd-bg;
display: flex;
flex-direction: column;
.woocommerce-product-gallery{
display: flex;
flex-direction: row-reverse;
position: relative;
a{
position: absolute;
z-index: 2;
top: 1rem;
right: 1rem;
}
div{
flex-grow: 1;
width: 100%;
height: 100%;
z-index: 1;
}
ol li{
list-style: none;
}
}

.pd-grid {
display: flex;
gap: $pd-gap;

@media (max-width: 1200px) {
grid-template-columns: 1fr;
}
}

.pd-left {
width: $l;
@media (max-width: 1200px) {
order: 1;
}

.woocommerce-product-gallery {
background: $pd-card;
border-radius: $pd-radius;
padding: 1rem;
box-shadow: $pd-shadow;
}

/* thumbs on left feel: just size tuning; Woo handles structure */
.flex-control-nav {
gap: .5rem;
}
}

.pd-summary {
width: $c;
@media (max-width: 1200px) {
order: 2;
}

color: $pd-text;

.pd-breadcrumbs {
color: $pd-soft;
margin-bottom: .25rem;
font-size: .85rem;
}

.pd-title {
font-size: 2rem;
margin: 0 0 .75rem;
}

.pd-meta {
margin: .5rem 0 1rem;

.pd-meta__title {
font-size: .95rem;
color: $pd-muted;
margin: 0 0 .35rem;
}

.pd-attrs {
list-style: none;
margin: 0;
padding: 0;
display: grid;
grid-template-columns: 1fr 1fr;
column-gap: $pd-gap;
row-gap: .4rem;
border-top: 1px solid $pd-border;

li {
display: contents;

.k {
color: $pd-soft;
padding: .55rem 0;
border-bottom: 1px solid $pd-border;
}

.v {
color: $pd-text;
padding: .55rem 0;
border-bottom: 1px solid $pd-border;
opacity: .95;
}
}
}
}

.pd-short {
color: $pd-muted;
line-height: 1.55;
}
}

.pd-buy {
width: $r;
@media (max-width: 1200px) {
order: 0;
}

position: relative;

.pd-card {
position: sticky;
top: $pd-sticky-top;
background: $pd-card;
border-radius: $pd-radius;
box-shadow: $pd-shadow;
padding: 1rem;
}

.pd-price {
display: flex;
align-items: baseline;
justify-content: space-between;
margin-bottom: .75rem;

.pd-sale-pct {
font-weight: 800;
font-size: .9rem;
background: $pd-accent;
color: #fff;
padding: .25rem .5rem;
border-radius: 999px;
}

.price {
display: inline-flex;
gap: .6rem;
align-items: baseline;

ins {
color: $pd-accent;
text-decoration: none;
font-size: 1.35rem;
font-weight: 800;
}

del {
color: $pd-oldprice;
font-size: 1rem;
}
}
}

/* Woo buttons */
form.cart {
display: grid;
gap: .6rem;

.quantity input {
height: 44px;
}

.single_add_to_cart_button {
height: 48px;
border-radius: 999px;
font-weight: 800;
background: $pd-accent;
border: 0;

&amp;:hover {
filter: brightness(1.05);
}
}

/* Your "Buy Now" hook button will sit just below */
.single_add_to_cart_button_alt,
.button.alt {
height: 44px;
border-radius: 999px;
font-weight: 800;
}
}
}

/* Similar items block */
.pd-similar {
margin-top: 2rem;

.pd-rowhead {
display: flex;
justify-content: space-between;
align-items: center;
margin-bottom: .75rem;

h3 {
color: $pd-text;
margin: 0;
}

.pd-more {
color: $pd-soft;
font-size: .9rem;
text-decoration: none;
}
}

/* Reuse your 3-up/4-up card styles from archive; Woo outputs .products gri=
d */
ul.products {
margin-top: .5rem;
}
}

/* Description */
.pd-desc {
margin: 1.5rem 0 2rem;

h3 {
color: $pd-text;
margin: 0 0 .5rem;
}

p,
li {
color: $pd-muted;
}
}
}

/* Tweak Woo defaults inside sticky box */
.d-product .pd-buy .variations,
.d-product .pd-buy .woocommerce-variation {
color: $pd-soft;
