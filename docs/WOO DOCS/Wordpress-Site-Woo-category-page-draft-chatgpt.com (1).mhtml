$pd-gap:        1rem;
$pd-left-w:     520px;    // gallery col
$pd-right-w:    340px;    // sticky buy
$pd-sticky:     80px;

$pd-radius:     16px;
$pd-shadow:     0 12px 36px rgba(0,0,0,.35);

$pd-bg:         #0f0f10;
$pd-card:       #141416;
$pd-text:       #fff;
$pd-muted:      rgba(255,255,255,.7);
$pd-soft:       rgba(255,255,255,.55);
$pd-border:     #242428;

$pd-accent:     #ff4da6;

$pd-thumb-w:    88px;
$pd-thumb-gap:  .5rem;

/* wrapper */
.site-main.d-product {
  background: $pd-bg;

  .pd-grid {
    display: flex;
    gap: $pd-gap;
    align-items: flex-start;
    flex-wrap: nowrap;

    @media (max-width:1200px){ flex-direction: column; }
  }

  /* LEFT: gallery */
  .pd-left {
    width: $pd-left-w; flex:0 0 $pd-left-w;
    @media (max-width:1200px){ width:100%; flex:0 0 auto; }

    .woocommerce-product-gallery{
      display: flex;
      flex-direction: row-reverse; /* thumbs first */
      gap: $pd-gap;
      background: $pd-card;
      border-radius: $pd-radius;
      padding: 1rem;
      box-shadow: $pd-shadow;
      align-items: stretch;

      .flex-viewport{
        height: auto !important;
        flex: 1 1 auto; min-width:0; overflow: visible;
      }

      .woocommerce-product-gallery__wrapper::after{
        content:""; display:block; clear:both;
      }

      .woocommerce-product-gallery__image,
      .woocommerce-product-gallery__image a{ display:block; width:100%; }
      .woocommerce-product-gallery__image img{
        display:block; width:100%; height:auto; border-radius:12px; object-=
fit:cover;
      }

      .flex-control-nav{
        display:flex; flex-direction:column;
        gap:$pd-thumb-gap;
        width:$pd-thumb-w; flex:0 0 $pd-thumb-w;
        margin:0; padding:0; list-style:none;
      }
      .flex-control-nav li{ list-style:none; }
      .flex-control-nav img{
        display:block; width:100%; height:auto; border-radius:12px;
        opacity:.9; transition:opacity .15s;
      }
      .flex-control-nav img:hover{ opacity:1; }
    }
  }

  /* CENTER: summary */
  .pd-summary{
    flex:1 1 auto; color:$pd-text;
    .pd-breadcrumbs{ color:$pd-soft; margin-bottom:.25rem; font-size:.85rem=
; }
    .pd-title{ font-size:2rem; margin:0 0 .75rem; }
    .pd-meta{ margin:.5rem 0 1rem;
      .pd-meta__title{ font-size:.95rem; color:$pd-muted; margin:0 0 .35rem=
; }
      .pd-attrs{
        display:grid; grid-template-columns:1fr 1fr;
        column-gap:$pd-gap; row-gap:0;
        margin:0; padding:0; list-style:none; border-top:1px solid $pd-bord=
er;
        li{ display:contents; }
        .k{ color:$pd-soft; padding:.55rem 0; border-bottom:1px solid $pd-b=
order; }
        .v{ color:$pd-text; padding:.55rem 0; border-bottom:1px solid $pd-b=
order; opacity:.95; }
      }
    }
    .pd-short{ color:$pd-muted; line-height:1.55; }
  }

  /* RIGHT: buy box */
  .pd-buy{
    width:$pd-right-w; flex:0 0 $pd-right-w;
    @media(max-width:1200px){ width:100%; flex:0 0 auto; }

    .pd-card{
      position:sticky; top:$pd-sticky;
      background:$pd-card; border-radius:$pd-radius;
      box-shadow:$pd-shadow; padding:1rem; color:$pd-text;
    }

    .pd-price{
      display:flex; justify-content:space-between; margin-bottom:.75rem;
      .price{
        display:inline-flex; gap:.6rem; align-items:baseline;
        ins{ color:$pd-accent; text-decoration:none; font-size:1.35rem; fon=
t-weight:800; }
        del{ color:rgba(255,255,255,.7); font-size:1rem; }
      }
    }

    form.cart{
      display:grid; gap:.6rem;
      .quantity input{ height:44px; }
      .single_add_to_cart_button{
        height:48px; border-radius:999px; font-weight:800;
        background:$pd-accent; border:0;
      }
      .single_add_to_cart_button_alt, .button.alt{
        height:44px; border-radius:999px; font-weight:800;
      }
    }
  }

  /* below */
  .pd-similar{ margin-top:2rem;
    .pd-rowhead{ display:flex; justify-content:space-between; align-items:c=
enter; margin-bottom:.75rem;
      h3{ color:$pd-text; margin:0; }
      .pd-more{ color:$pd-soft; text-decoration:none; font-size:.9rem; }
    }
  }
  .pd-desc{ margin:1.5rem 0 2rem;
    h3{ color:$pd-text; margin:0 0 .5rem; }
    p,li{ color:$pd-muted; }
  }
