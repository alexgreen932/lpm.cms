//curent css/* =3D=3D=3D=3D=3D=3D=3D Single Product =E2=80=93 Flex Layout =
=3D=3D=3D=3D=3D=3D=3D */
$pd-gap:        1rem;
$pd-left-w:     520px;   // left column (gallery)
$pd-right-w:    340px;   // sticky buy box
$pd-sticky:     80px;

$pd-radius:     16px;
$pd-shadow:     0 12px 36px rgba(0,0,0,.35);
$pd-card:       #141416;
$pd-text:       #fff;
$pd-soft:       rgba(255,255,255,.55);
$pd-muted:      rgba(255,255,255,.70);
$pd-border:     #242428;
$pd-accent:     #ff4da6;

$pd-thumb-w:    88px;    // thumbs column width
$pd-thumb-gap:  .5rem;   // &lt;=E2=80=94 requested gap

.site-main.d-product {
  /* top band with 3 columns using flex */
  .pd-grid {
    display: flex;
    gap: $pd-gap;
    align-items: flex-start;
    flex-wrap: nowrap;

    @media (max-width: 1200px) {
      flex-direction: column;
    }
  }

  /* LEFT: gallery */
  .pd-left {
    width: $pd-left-w;
    flex: 0 0 $pd-left-w;

    @media (max-width: 1200px) { width: 100%; flex: 0 0 auto; }

    /* Woo gallery wrapper */
    .woocommerce-product-gallery {
      display: flex;
      flex-direction: row-reverse;  /* &lt;=E2=80=94 thumbs first on the le=
ft */
      gap: $pd-gap;
      background: $pd-card;
      border-radius: $pd-radius;
      padding: 1rem;
      box-shadow: $pd-shadow;
      overflow: hidden;
       div{
    flex-grow: 1;
    width: 100%;
    height: 100%;  // =E2=86=90 this is the killer
    z-index: 1;
  }

      /* main image area */
      .woocommerce-product-gallery__wrapper {
        /* make sure main image doesn't disappear */
        flex: 1 1 auto;
        min-width: 0; /* allow shrink */
        figure,
        .woocommerce-product-gallery__image {
          width: 100%;
        }
        img {
          display: block;
          width: 100%;
          height: auto;
          border-radius: 12px;
        }
      }

      /* thumbs column (Woo: ol.flex-control-nav.flex-control-thumbs) */
      .flex-control-nav {
        display: flex;
        flex-direction: column;
        gap: $pd-thumb-gap;
        width: $pd-thumb-w;
        flex: 0 0 $pd-thumb-w;
        margin: 0; padding: 0; list-style: none;

        li { list-style: none; }
        img {
          display: block;
          width: 100%;
          height: auto;
          border-radius: 12px;
          opacity: .9;
          transition: opacity .15s ease;
          &amp;:hover { opacity: 1; }
        }
      }

      /* optional: the zoom icon you absolutely positioned before */
      a.zoomImg, .pswp__button {
        position: absolute;
        right: 1rem; top: 1rem; z-index: 2;
      }
    }
  }

  /* CENTER: summary */
  .pd-summary {
    flex: 1 1 auto;
    color: $pd-text;

    .pd-breadcrumbs { color: $pd-soft; margin-bottom: .25rem; font-size: .8=
5rem; }
    .pd-title { font-size: 2rem; line-height: 1.15; margin: 0 0 .75rem; }

    /* two-column attributes table */
    .pd-meta {
      margin: .5rem 0 1rem;

      .pd-meta__title { font-size: .95rem; color: $pd-muted; margin: 0 0 .3=
5rem; }

      .pd-attrs {
        display: grid;
        grid-template-columns: 1fr 1fr;
        column-gap: $pd-gap; row-gap: 0;
        margin: 0; padding: 0; list-style: none;
        border-top: 1px solid $pd-border;

        li { display: contents; }
        .k { color: $pd-soft; padding: .55rem 0; border-bottom: 1px solid $=
pd-border; }
        .v { color: $pd-text; padding: .55rem 0; border-bottom: 1px solid $=
pd-border; opacity: .95; }
      }
    }

    .pd-short { color: $pd-muted; line-height: 1.55; }
  }

  /* RIGHT: sticky buy card */
  .pd-buy {
    width: $pd-right-w;
    flex: 0 0 $pd-right-w;
    position: relative;

    @media (max-width: 1200px) { width: 100%; flex: 0 0 auto; }

    .pd-card {
      position: sticky; top: $pd-sticky;
      background: $pd-card; border-radius: $pd-radius;
      box-shadow: $pd-shadow; padding: 1rem;
      color: $pd-text;
    }

    .pd-price {
      display: flex; align-items: baseline; justify-content: space-between;=
 margin-bottom: .75rem;
      .pd-sale-pct { font-weight: 800; font-size: .9rem; background: $pd-ac=
cent; color: #fff; padding: .25rem .5rem; border-radius: 999px; }
      .price { display: inline-flex; gap: .6rem; align-items: baseline;
        ins { color: $pd-accent; text-decoration: none; font-size: 1.35rem;=
 font-weight: 800; }
        del { color: rgba(255,255,255,.7); font-size: 1rem; }
      }
    }

    form.cart {
      display: grid; gap: .6rem;
      .quantity input { height: 44px; }
      .single_add_to_cart_button {
        height: 48px; border-radius: 999px; font-weight: 800; background: $=
pd-accent; border: 0;
        &amp;:hover { filter: brightness(1.05); }
      }
      .single_add_to_cart_button_alt, .button.alt {
        height: 44px; border-radius: 999px; font-weight: 800;
      }
    }
  }

  /* sections below */
  .pd-similar { margin-top: 2rem;
    .pd-rowhead { display:flex; justify-content:space-between; align-items:=
center; margin-bottom:.75rem;
      h3{margin:0;color:$pd-text;} .pd-more{color:$pd-soft; text-decoration=
:none; font-size:.9rem;}
    }
  }
  .pd-desc { margin: 1.5rem 0 2rem;
    h3{color:$pd-text; margin:0 0 .5rem;} p,li{color:$pd-muted;}
  }
}

/* a tiny hardening: Woo defaults sometimes set max-width on gallery image =
container */
.woocommerce-product-gallery__wrapper { max-width: 100% !important; }
